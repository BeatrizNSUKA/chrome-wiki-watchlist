{
  "manifest_version": 2,
  "name": "The Old Reader",
  "description": "Notifies you about unread items in your The Old Reader account",
  "version": "0.4",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [ "functions.js", "background.js" ]
  },
  "browser_action": {
    "default_icon": "icon-inactive.png"
  },
  "permissions": [
    "tabs",
    "http://theoldreader.com/"
  ]
}
